<h1>Welcome to Wikinimous</h1>
<%= link_to "CREATE ARTICLE", new_article_path %>
<ul>
  <% @articles.each do |article|%>
   <li>
      <h2><%= link_to article.title, article_path(article) %></h2>
      <p><%= article.content %></p>
      <%= link_to "DELETE ARTICLE", article_path(article), method: :delete, data: {confirm: "are you sure you want to delete?"} %>
      <%= link_to "EDIT ARTICLE", edit_article_path(article) %>
   </li>
  <% end %>
</ul>
